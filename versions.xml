<?xml version="1.0" encoding="utf-8"?>
<extension version="1.7" type="plugin" group="editors-xtd">
	<name>PLG_EDITORS-XTD_TITLE</name>
	<author>Ronald J. de Vries</author>
	<creationDate>August 2011</creationDate>
	<license>GNU General Public License version 2 or later; see	LICENSE.txt</license>
	<authorEmail>post@rjdevries.eu</authorEmail>
	<authorUrl>http://www.rjdev.nl</authorUrl>
	<version>1.1</version>
	<description>PLG_EDITORS-XTD_DESC</description>
	<files>
		<filename plugin="versions">versions.php</filename>
	</files>
	
	<languages folder="languages">
		<language tag="en-GB">en-GB.plg_editors-xtd_versions.ini</language>
                <language tag="en-GB">en-GB.plg_editors-xtd_versions.sys.ini</language>
		<language tag="nl-NL">nl-NL.plg_editors-xtd_versions.ini</language>
                <language tag="nl-NL">nl-NL.plg_editors-xtd_versions.sys.ini</language>
        </languages>
	
	<config>
	<fields name="params">
		<fieldset name="basic">
			<field name="popup_width"
				type="text"
				default="450"
				label="PLG_EDITORS-XTD_POPUP_WIDTH_LABEL"
				description="PLG_EDITORS-XTD_POPUP_WIDTH_DESC" >
				</field>
			<field name="popup_height"
				type="text"
				default="370"
				label="PLG_EDITORS-XTD_POPUP_HEIGHT_LABEL"
				description="PLG_EDITORS-XTD_POPUP_HEIGHT_DESC" >
				</field>
			</fieldset>
		</fields>
	</config>
</extension>
